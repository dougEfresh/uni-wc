{
  "name": "uni-wc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:all": "pnpm -r run build",
    "web:dev": "pnpm -F cctp run dev",
    "web:build": "pnpm -F cctp run build",
    "web:start": "pnpm -F cctp run start",
    "cli": "pnpm build:pack && pnpm -F cli run cli",
    "build": "pnpm build:pack && pnpm build:types",
    "build:pack": "pnpm -r --filter './packages/**' run build",
    "build:types": "pnpm -r --filter './packages/**' run build:types",
    "dev:webapp": "pnpm -F webapp run dev",
    "test:libs": "pnpm -r --filter ./packages run test",
    "test:integration": "pnpm build:pack && pnpm -r --filter ./packages/integration/ run test:integration",
    "clean": "pnpm -r run clean",
    "distclean": "pnpm clean; rm -rf node_modules packages/*/node_modules packages/*/*/node_modules apps/*/node_modules"
  },
  "pnpm": {
    "overrides": {
      "@walletconnect/sign-client": "$@walletconnect/sign-client",
      "@walletconnect/universal-provider": "$@walletconnect/universal-provider"
    }
  },
  "dependencies": {
    "@fireblocks/ts-sdk": "^2.1.0",
    "@jup-ag/api": "^6.0.24",
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.95.1",
    "@solendprotocol/solend-sdk": "^0.8.8",
    "@walletconnect/core": "2.13.3",
    "@walletconnect/keyvaluestorage": "^1.1.1",
    "@walletconnect/logger": "^2.1.2",
    "@walletconnect/sign-client": "2.13.3",
    "@walletconnect/universal-provider": "2.13.3",
    "@walletconnect/utils": "2.13.3",
    "@wormhole-foundation/sdk": "^0.8.2",
    "@wormhole-foundation/sdk-definitions": "^0.8.2",
    "bs58": "^5.0.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.2",
    "next": "14.2.3",
    "pino": "^7.11.0",
    "pino-pretty": "^11.2.2",
    "qrcode-terminal": "^0.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "unstorage": "^1.10.2",
    "viem": "2.18.2",
    "wagmi": "2.12.1",
    "xdg-basedir": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@walletconnect/types": "2.13.3",
    "babel-jest": "^29.7.0",
    "babel-plugin-add-import-extension": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
